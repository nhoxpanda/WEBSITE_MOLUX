@model IEnumerable<TOURDEMO.Models.HistoryLogViewModel>
@{
    int i = 1;
}

<div class="table-scrollable table-custom-format" id="tableDictionary-Wrapper">
    <table class="table table-bordered table-hover dataTable" id="tableDictionary">
        <thead>
            <tr>
                <td class="width24"></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            <tr>
                <th class="table-checkbox">
                    <input type="checkbox" id="allcb" name="allcb" onclick="SelectAllCb()" />
                </th>
                <th>STT</th>
                <th>Module</th>
                <th>Form</th>
                <th>Ngày xử lý</th>
                <th>Diễn giải</th>
                <th>Nhân viên</th>
            </tr>
        </thead>
        <tbody>
            @if (Model.Count() > 0)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td class="width24">
                            <input type="checkbox" value="@item.Id" id="cb@(item.Id)" class="cbItem" name="cb" onclick="CheckSelect()" />
                        </td>
                        <td style="text-align: center">@i</td>
                        <td>@item.Module</td>
                        <td>@item.Form</td>
                        <td>@item.Date</td>
                        <td>@item.Note</td>
                        <td>@item.Staff</td>
                    </tr>
                    i++;
                }
            }
        </tbody>
    </table>
</div>